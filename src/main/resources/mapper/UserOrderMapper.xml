<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.business.mapper.UserOrderPOMapper">


    <select id="getOrderByDesc" resultType="com.example.business.entity.UserOrderPO">
         select
              *
         FROM
              user_order
         ORDER BY
              order_id
         DESC
    </select>


</mapper>
